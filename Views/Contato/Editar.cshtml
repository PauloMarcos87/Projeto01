@{
    ViewData["Title"] = "Editar Contato";
}

<div class="text-center">

    <h1 class="display-4">Editar Contato</h1>

</div>


<form>
    <div class="form-group">
        <label for="nome">Nome</label>
        <input type="texto" class="form-control" placeholder="Digete o Nome do contato">
    </div>

    <div class="form-group">
        <label for="email">E-mail</label>
        <input type="texto" class="form-control" placeholder="Digete o E-mail do contato">
    </div>

    <div class="form-group">
        <label for="contato">Contato</label>
        <input type="texto" class="form-control" placeholder="Digete o Contato">
    </div>


    <button type="submit" class="btn btn-primary">Alterar</button>
    <a class="btn btn-secondary" asp-controller="Contato" asp-action="Index" role="button">Voltar</a>
</form>